{
    "name": "jerowork/middleware-request-handler",
    "type": "library",
    "description": "Minimalist PSR-15 middleware based request handler (dispatcher)",
    "keywords": [
        "PSR-15",
        "PSR-7",
        "middleware",
        "request handler",
        "dispatcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen de Graaf",
            "email": "info@jero.work",
            "homepage": "https://jero.work"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "provide": {
        "psr/http-server-handler": "1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0",
        "zendframework/zend-diactoros": "^1.7 || ^2.0"
    },
    "suggest": {
        "zendframework/zend-diactoros": "As PSR-7 implementation"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jerowork\\MiddlewareDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jerowork\\MiddlewareDispatcher\\Test\\": "tests/"
        }
    }
}
