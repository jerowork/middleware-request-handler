{
    "name": "jerowork/middleware-request-handler",
    "type": "library",
    "description": "Minimalist PSR-15 middleware based request handler (dispatcher)",
    "keywords": [
        "PSR-15",
        "PSR-7",
        "middleware",
        "request handler",
        "dispatcher"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen de Graaf",
            "email": "jeroen.graaf@gmail.com",
            "homepage": "http://jero.work"
        }
    ],
    "require": {
        "php": "^7.0",
        "http-interop/http-server-handler": "^1.0",
        "http-interop/http-server-middleware": "^1.0"
    },
    "provide": {
        "http-interop/http-server-handler": "1.0"
    },
    "suggest": {
        "zendframework/zend-diactoros": "As PSR-7 implementation"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Jerowork\\MiddlewareDispatcher\\": "src/"
        }
    }
}
